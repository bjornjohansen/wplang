{
  "name": "bjornjohansen/wplang",
  "description": "Composer plugin to download translation files from wordpress.org",
  "license": "GPL-2.0+",
  "type": "composer-plugin",
  "authors": [
    {
      "name": "BjÃ¸rn Johansen",
      "email": "post@bjornjohansen.no"
    },
    {
      "name": "Alexander Goller",
      "email": "alpipego@protonmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "composer-plugin-api": "^2.0"
  },
  "require-dev": {
    "ext-json": "*",
    "composer/composer": "^2.0",
    "yoast/phpunit-polyfills": "^3.1",
    "squizlabs/php_codesniffer": "^3.11",
    "wp-coding-standards/wpcs": "^3.1",
    "slevomat/coding-standard": "^8.15",
    "phpcompatibility/php-compatibility": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "BJ\\Wplang\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BJ\\Wplang\\Tests\\": "tests"
    }
  },
  "extra": {
    "class": "BJ\\Wplang\\Wplang"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
